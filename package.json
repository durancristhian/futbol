    "react-scripts": "1.1.5",
    "react-tilt": "^0.1.4",
    "stylelint": "^9.4.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "tachyons": "^4.11.1"
  },
  "dependencies": {
